{
  "name": "番茄七猫小说下载器",
  "short_name": "番茄七猫",
  "description": "支持番茄小说和七猫小说的批量下载和EPUB导出工具",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "background_color": "#ffffff",
  "theme_color": "#3b82f6",
  "orientation": "portrait-primary",
  "lang": "zh-CN",
  "dir": "ltr",
  "categories": ["books", "productivity", "utilities"],
  "icons": [
    {
      "src": "/static/images/icon-64.png",
      "sizes": "64x64",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/static/images/icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/static/images/icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/static/images/icon.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any"
    }
  ],
  "shortcuts": [
    {
      "name": "搜索书籍",
      "short_name": "搜索",
      "description": "搜索番茄和七猫小说",
      "url": "/search",
      "icons": [
        {
          "src": "/static/images/icon-64.png",
          "sizes": "64x64"
        }
      ]
    },
    {
      "name": "我的书籍",
      "short_name": "书籍",
      "description": "查看已添加的书籍",
      "url": "/books",
      "icons": [
        {
          "src": "/static/images/icon-64.png",
          "sizes": "64x64"
        }
      ]
    },
    {
      "name": "下载任务",
      "short_name": "任务",
      "description": "查看下载进度",
      "url": "/tasks",
      "icons": [
        {
          "src": "/static/images/icon-64.png",
          "sizes": "64x64"
        }
      ]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+fanqie",
      "url": "/search?q=%s"
    }
  ],
  "share_target": {
    "action": "/search",
    "method": "GET",
    "params": {
      "text": "q"
    }
  }
}
